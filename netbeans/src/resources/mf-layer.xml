<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN"
          "http://www.netbeans.org/dtds/filesystem-1_1.dtd">

<filesystem>
  <folder name="Menu">
    <folder name="Build">

      <!-- add the Jalopy menu item 'Format'
           position after 'Clean All' -->
      <attr name="org-openide-actions-CleanAllAction.instance/de-hunsicker-jalopy-plugin-netbeans-FormatAction.instance"
            boolvalue="true"/>
      <file name="de-hunsicker-jalopy-plugin-netbeans-FormatAction.instance"/>

      <!-- add separator
           position after 'Format' -->
      <attr name="de-hunsicker-jalopy-plugin-netbeans-FormatAction.instance/mymodule-sep[javax-swing-JSeparator].instance"
            boolvalue="true"/>
      <file name="mymodule-sep[javax-swing-JSeparator].instance"/>
    </folder>

    <folder name="Tools">

      <!-- add the Jalopy menu item 'Jalopy Options...' 
            and position after 'Options...' -->
      <attr name="org-netbeans-core-actions-OptionsAction.instance/de-hunsicker-jalopy-plugin-netbeans-PreferencesAction.instance"
            boolvalue="true" />
      <file name="de-hunsicker-jalopy-plugin-netbeans-PreferencesAction.instance"/>
      <attr name="de-hunsicker-jalopy-plugin-netbeans-PreferencesAction.instance/Separator3[javax-swing-JSeparator].instance"
            boolvalue="true"/>
    </folder>
  </folder>

  <folder name="System">
    <file name="de-hunsicker-jalopy-plugin-netbeans-FormatAction.instance"/>
    <file name="de-hunsicker-jalopy-plugin-netbeans-PreferencesAction.instance"/>
  </folder>

  <folder name="Shortcuts">
    <file name="CS-F10[de-hunsicker-jalopy-plugin-netbeans-FormatAction].instance" />
  </folder>

  <folder name="Editors">
    <folder name="AnnotationTypes">
      <file name="de-hunsicker-jalopy-plugin-netbeans-error.xml"
            url="error-annotation.xml"/>
      <file name="de-hunsicker-jalopy-plugin-netbeans-warn.xml"
            url="warn-annotation.xml"/>
    </folder>
  </folder>
</filesystem>