            <removeSingleLine>false</removeSingleLine>
            <formatMultiLine>false</formatMultiLine>
            <removeMultiLine>false</removeMultiLine>
            <insertSeparatorRecursive>false</insertSeparatorRecursive>
        </comments>
        <footer>
            <keys />
            <text></text>
            <use>false</use>
            <smartModeLines>0</smartModeLines>
