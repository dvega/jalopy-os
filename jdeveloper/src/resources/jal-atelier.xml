            <grouping>javax:1|java:1|gnu:2|com.borland:2|org.apache:2|de.hunsicker:2|org.openide:2|org.eclipse:2</grouping>
            <groupingDepth>3</groupingDepth>
            <sort>true</sort>
        </import>
